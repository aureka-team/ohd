<% content = FasterCSV.generate(:col_sep => "\t") do |csv| -%>
  <% @list.each do |col| -%>
    <% row = @rows[col] -%>
    <% if row === nil -%>
      <% csv << [ col ] -%>
    <% else -%>
      <% cells_for_row = [ row[:label], row[:sum] ] -%>
      <% @month_labels.each do |month_label| -%>
        <% cells_for_row << row[:cols][month_label] -%>
      <% end -%>
      <% csv << cells_for_row -%>
    <% end -%>
  <% end -%>
  <% csv << [] -%>
  <% csv << [ "Stand: #{Time.now.strftime("%d.%m.%Y %H:%M")}" ] -%>
  <% csv << [ "Zeitraum: Gesamt" ] -%>
<% end -%>
<%= content -%>
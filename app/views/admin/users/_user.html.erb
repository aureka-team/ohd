<li id="user_<%= user.id -%>" class="user">
  <% last_login = user.user_account.last_sign_in_at || Time.gm(2009,1,1) -%>
  <span class="name"><%= user.last_name -%>, <%=  user.first_name -%><br/>
    <span class="email" style="font-size: 85%;"><%= user.email -%></span>
  </span>
  <span class="login"><%=  user.login -%></span>
            <span class="data last_login">
              <span style="display: none;">
                <%= last_login -%>
              </span>
              <%= formatted_time last_login -%>
            </span>
  <%= render :partial => 'admin_flag', :object => user %>
</li>
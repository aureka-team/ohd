<%# This is the playlist for the player preview -%>
<%# cache [@tapes.first.interview.to_param, 'test_playlist.xml'].join('/') do -%>
<?xml version="1.0" encoding="utf-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">
  <tracklist>
    <% @tapes.each do |tape| -%>
      <track>
        <location><%= tape.media_id %></location>
        <meta rel="captions"><%= transcript_interview_tape_path(:id => tape.id, :format => :xml) %></meta>
        <meta rel="start">10</meta>
      </track>
    <% end -%>
  </tracklist>
</playlist>
<%# end -%>
<%= render partial: "./latex/title",
           locals: {
               title: latex_escape(I18n.backend.translate(@locale,'legend.title')),
               space_above: 0,
               space_below: 0.01,
               font_size: 13
           } %>
<% if @interview.project.identifier.to_sym == :mog %>
\par{
  \begin{description}[leftmargin=4em,style=nextline]
    \item[\normalfont{( )}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.missed')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[.]}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.comment')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(-)}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.pause_1')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(--)}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.pause_2')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(---)}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.pause_3')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(4)}] Text.<%= latex_escape(I18n.backend.translate(@locale,'legend.mog.pause_4')) %>
    \vspace*{0.2cm}
    \item[\normalfont{\_}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.assert')) %>
    \vspace*{0.2cm}
    \item[\normalfont{'}] <%= latex_escape(I18n.backend.translate(@locale,'legend.mog.omission')) %>
  \end{description}
}
<% else %>
\par{
  \begin{description}[leftmargin=4em,style=nextline]
    \item[\normalfont{[...]}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.address')) %>
    \vspace*{0.2cm}
    \item[\normalfont{...}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.broken_word')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[---]}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.silence')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(???)}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.unclear')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(<%= I18n.backend.translate(@locale, :word) %> ?)}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.unsure')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[x]}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.break')) %>
    \vspace*{0.2cm}
    \item[\textit{<%= I18n.backend.translate(@locale, :italic) %>}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.italic')) %>
    \vspace*{0.2cm}
    \item[\normalfont{"xyz"}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.quote')) %>
    \vspace*{0.2cm}
    \item[\normalfont{{<%= I18n.backend.translate(@locale, :text) %>} <%= I18n.backend.translate(@locale, :or) %> {(<%= I18n.backend.translate(@locale, :text) %>)}}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.annotation')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<***>}] <%= latex_escape(I18n.backend.translate(@locale,'legend.zwar.tape_end')) %>
    \vspace*{0.2cm}
  \end{description}
}
<% end %>


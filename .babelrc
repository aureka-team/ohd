{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          // "browsers": "> 1%"

          // https://stackoverflow.com/a/45212317
          "browsers": ["last 2 versions", "ie >= 11"]
        },
        "useBuiltIns": "entry",
        "forceAllTransforms": true
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
